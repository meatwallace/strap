{
  "name": "strap",
  "version": "0.0.0",
  "description": "Javascript monorepo boilerplate",
  "scripts": {
    "start": "node main.js",
    "deploy:browser": "npm run build:browser:prod; now dist/browser -n 'strap-client'",
    "deploy:server": "npm run build:server:prod; cp package.json now.json dist/server; now dist/server",
    "build:browser:dev": "webpack-dev-server --progress --env 'browser.dev'",
    "build:browser:prod": "webpack --progress --env 'browser.prod'",
    "build:server:dev": "webpack --watch --progress --env 'server' & nodemon ./dist/server/main.js",
    "build:server:prod": "webpack --progress --env 'server'",
    "build:ios:dev": "react-native run-ios",
    "build:android:dev": "react-native run-android",
    "test": "jest --watch --coverage",
    "test-ci": "jest --bail --coverage",
    "lint": "prettier-eslint src/**/*.js --write; stylefmt -r src/**/*.css",
    "clean-cache": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meatwallace/strap.git"
  },
  "author": "Geoff Whatley",
  "license": "MIT",
  "homepage": "https://github.com/meatwallace/strap#readme",
  "engines": {
    "node": "6.x.x"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-native": "1.9.1",
    "babel-preset-stage-0": "^6.3.13",
    "commitizen": "^2.9.6",
    "css-loader": "^0.27.0",
    "cz-conventional-changelog": "^2.0.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.4.3",
    "enzyme": "^2.1.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-prettier": "^1.5.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^19.0.2",
    "jest-transform-graphql": "^1.0.0",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-modular-scale": "^2.5.2",
    "prettier": "^0.22.0",
    "prettier-eslint": "^4.2.0",
    "prettier-eslint-cli": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-mock-store": "1.2.2",
    "request": "^2.80.0",
    "sinon": "^2.0.0",
    "style-loader": "^0.15.0",
    "stylefmt": "^5.2.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "0.4.1",
    "svg-url-loader": "^2.0.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.0.0"
  },
  "dependencies": {
    "analytics-react-native": "^1.1.0",
    "apollo-client": "^1.0.0-rc.1",
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "bcrypt-as-promised": "^1.1.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "feathers": "^2.1.1",
    "feathers-authentication": "^1.1.1",
    "feathers-authentication-jwt": "^0.3.1",
    "feathers-authentication-local": "^0.3.3",
